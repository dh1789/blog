{
  "name": "wordpress-publishing-cli",
  "version": "0.1.0",
  "private": true,
  "description": "WordPress + Avada 블로그 마크다운 콘텐츠 발행 및 관리 CLI 도구",
  "keywords": [
    "wordpress",
    "publishing",
    "blog",
    "cli",
    "avada",
    "content-management",
    "markdown"
  ],
  "author": "idongho",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter @blog/cli dev",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "1.6.1",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.55.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  }
}
